    if specials and sweets and drysnacks and khakharas:
        pass
    else:
        addingAllDrySnacks()
        addingAllKhakharas()
        addingAllSpecials()
        addingAllSweets()
        db.session.commit()
    specials = SpecialsProduct.query.all()
    sweets = SweetsProduct.query.all()
    drysnacks = DrySnacksProduct.query.all()
    khakharas = KhakharasProduct.query.all()

